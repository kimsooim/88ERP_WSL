# 88ERP Docker 환경 설정 예시
# 이 파일을 .env로 복사하여 사용하세요

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/88erp
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=88erp

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here-change-this

# Redis
REDIS_URL=redis://localhost:6379

# Node
NODE_ENV=development

# 포트 설정
WEB_DASHBOARD_PORT=3000
MONITOR_PORT=3001
DB_PORT=5433
REDIS_PORT=6379

# 운영 서버 정보 (참고용)
PRODUCTION_SERVER_IP=192.168.32.128
PRODUCTION_SERVER_USER=root